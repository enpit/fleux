{
    "env": {
        "development": {
            "presets": [
                ["@babel/preset-env", { "modules": false }],
                "@babel/preset-react"
            ],
            "plugins": [
                ["@babel/plugin-proposal-decorators", {"legacy": true}],
                ["@babel/plugin-proposal-class-properties", {"loose":true}],
                "@babel/plugin-transform-react-jsx-source"
            ]
        },
        "test": {
            "presets": [
                ["@babel/preset-env", { "modules": false }],
                "@babel/preset-react"
            ],
            "plugins": [
                ["@babel/plugin-proposal-decorators", {"legacy": true}],
                ["@babel/plugin-proposal-class-properties", {"loose":true}],
                "@babel/plugin-transform-react-jsx-source"
            ]
        },
        "production": {
            "presets": [
                ["@babel/preset-env", { "modules": false }],
                "@babel/preset-react"
            ],
            "plugins": [
                ["@babel/plugin-proposal-decorators", {"legacy": true}],
                ["@babel/plugin-proposal-class-properties", {"loose":true}],
            ]
        }
    }
}
